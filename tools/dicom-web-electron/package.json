{
    "build": {
        "appId": "dicom-web-electron",
        "mac": {
            "category": "development"
        },
        "win": {
            "target": [
                {
                    "target": "portable"
                }
            ]
        },
        "nsis": {
            "useZip": true
        }
    },
    "name": "dicom-web-electron",
    "version": "1.0.0",
    "description": "An electron client for Dicom Web",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "pack": "electron-builder --dir",
        "dist": "electron-builder"
    },
    "author": "Microsoft",
    "license": "ISC",
    "devDependencies": {
        "electron": "^9.4.0",
        "electron-builder": "^22.9.1"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "form-data": "^3.0.0",
        "jquery": "^3.5.1"
    }
}
